class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """

        #哈希映射
        #num->index
        #O(n)
        
        hashmap = {}
        n = len(nums)
        for i in range(n):
            tmp = target - nums[i]
            if hashmap.has_key(tmp):
                return [i, hashmap.get(tmp)]
            hashmap[nums[i]] = i
