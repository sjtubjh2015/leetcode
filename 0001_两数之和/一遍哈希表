class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """


        #哈希映射
        #num->index
        O(n)
        
        
        hashmap = {}
        for i in range(len(nums)):
            if hashmap.has_key(target - nums[i]):
                return [i, hashmap.get(target - nums[i])]
            hashmap[nums[i]] = i
