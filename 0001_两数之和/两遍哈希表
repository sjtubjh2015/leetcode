class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """

        #两遍哈希表
        #num->index
        #O(n)

        hashmap = {}
        n = len(nums)
        for i in range(n):
            hashmap[nums[i]] = i
        for i in range(n):
            tmp = target - nums[i]
            if hashmap.has_key(tmp) and i != hashmap.get(tmp):
                return [i, hashmap.get(tmp)]
