class Solution(object):
    def moveZeroes(self, nums):
        """
        :type nums: List[int]
        :rtype: None Do not return anything, modify nums in-place instead.
        """

        cur = 0
        n = len(nums)
        for i in range(n):
            if nums[i] != 0:
                nums[cur] = nums[i]
                cur += 1
        for i in range(cur, n):
            nums[i] = 0
