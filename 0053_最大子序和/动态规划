class Solution(object):
    def maxSubArray(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """

        n = len(nums)
        maxSub = nums[0]
        dp = nums[0]
        for i in range(1, n):
            dp = max(dp + nums[i], nums[i])
            if dp > maxSub:
                maxSub = dp
        return maxSub
